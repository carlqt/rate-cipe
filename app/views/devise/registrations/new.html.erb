<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
	<%= devise_error_messages! %>

	  <div><%= f.label :first_name%><br />
	  <%= f.text_field :first_name, :autofocus => true %><br /><br /></div>

	  <div><%= f.label :last_name%><br />
	  <%= f.text_field :last_name %><br /><br /></div>

	  <div><%= f.label :username %><br />
	  <%= f.text_field :username%><br /><br /></div>

	  <div><%= f.label :email %><br />
	  <%= f.email_field :email %><br /><br /></div>

	  <div><%= f.label :password %><br />
	  <%= f.password_field :password %><br /><br /></div>

	  <div><%= f.label :password_confirmation %><br />
	  <%= f.password_field :password_confirmation %><br /><br /></div>

	  <div><%= f.submit "Sign up" %></div><br>
<% end %>

<%= render "devise/shared/links" %>
